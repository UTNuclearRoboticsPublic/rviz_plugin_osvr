material Ogre/Compositor/Osvr
{
  technique
  {
    pass
    {
      depth_check off

      vertex_program_ref rviz_plugin_osvr/glsl120/warp.vert {}
      fragment_program_ref rviz_plugin_osvr/glsl120/warp.frag {}
      
      texture_unit RT
      {
        tex_coord_set 0
        tex_address_mode border
        tex_border_colour 0 0 0
        filtering linear linear linear
      }
    }
  }
}
